@using Microsoft.AspNet.Identity;
@model Squash.Models.DashboardViewModel

<main id="main-container">
    <div class="content bg-image overflow-hidden" style="background-image: url('/Images/photos/photo3@2x.jpg');">
        <div class="push-50-t push-15">
            <h1 class="h2 text-white animated zoomIn">Dashboard</h1>
            <h2 class="h5 text-white-op animated zoomIn">Welcome @Model.FullName</h2>
        </div>
    </div>
    <div class="content">
        @if (User.IsInRole("Administrator"))
        {
        <div class="row">
            <div class="col-lg-4">
                <div class="block">
                    <div class="block-header">
                        <ul class="block-options">
                            <li>
                                <button type="button" data-toggle="block-option" data-action="refresh_toggle" data-action-mode="demo"><i class="si si-refresh"></i></button>
                            </li>
                        </ul>
                        <h3 class="block-title">Ticket Status Statistics</h3>
                    </div>
                    <div class="block-content block-content-full text-center">
                        <div class="chartjs-size-monitor" style="position: absolute; left: 0px; top: 0px; right: 0px; bottom: 0px; overflow: hidden; pointer-events: none; visibility: hidden; z-index: -1;"><div class="chartjs-size-monitor-expand" style="position:absolute;left:0;top:0;right:0;bottom:0;overflow:hidden;pointer-events:none;visibility:hidden;z-index:-1;"><div style="position:absolute;width:1000000px;height:1000000px;left:0;top:0"></div></div><div class="chartjs-size-monitor-shrink" style="position:absolute;left:0;top:0;right:0;bottom:0;overflow:hidden;pointer-events:none;visibility:hidden;z-index:-1;"><div style="position:absolute;width:200%;height:200%;left:0; top:0"></div></div></div>
                        <!-- Donut Chart Container -->
                        <canvas id="statusChart" class="js-chartjs2-donut chartjs-render-monitor" width="558" height="279" style="display: block; width: 558px; height: 279px;"></canvas>
                    </div>
                </div>
            </div>
            <div class="col-lg-4">
                <div class="block">
                    <div class="block-header">
                        <ul class="block-options">
                            <li>
                                <button type="button" data-toggle="block-option" data-action="refresh_toggle" data-action-mode="demo"><i class="si si-refresh"></i></button>
                            </li>
                        </ul>
                        <h3 class="block-title">Ticket Priority Statistics</h3>
                    </div>
                    <div class="block-content block-content-full text-center">
                        <div class="chartjs-size-monitor" style="position: absolute; left: 0px; top: 0px; right: 0px; bottom: 0px; overflow: hidden; pointer-events: none; visibility: hidden; z-index: -1;"><div class="chartjs-size-monitor-expand" style="position:absolute;left:0;top:0;right:0;bottom:0;overflow:hidden;pointer-events:none;visibility:hidden;z-index:-1;"><div style="position:absolute;width:1000000px;height:1000000px;left:0;top:0"></div></div><div class="chartjs-size-monitor-shrink" style="position:absolute;left:0;top:0;right:0;bottom:0;overflow:hidden;pointer-events:none;visibility:hidden;z-index:-1;"><div style="position:absolute;width:200%;height:200%;left:0; top:0"></div></div></div>
                        <!-- Donut Chart Container -->
                        <canvas id="priorityChart" class="js-chartjs2-donut chartjs-render-monitor" width="558" height="279" style="display: block; width: 558px; height: 279px;"></canvas>
                    </div>
                </div>
            </div>
            <div class="col-lg-4">
                <div class="block">
                    <div class="block-header">
                        <ul class="block-options">
                            <li>
                                <button type="button" data-toggle="block-option" data-action="refresh_toggle" data-action-mode="demo"><i class="si si-refresh"></i></button>
                            </li>
                        </ul>
                        <h3 class="block-title">Ticket Type Statistics</h3>
                    </div>
                    <div class="block-content block-content-full text-center">
                        <div class="chartjs-size-monitor" style="position: absolute; left: 0px; top: 0px; right: 0px; bottom: 0px; overflow: hidden; pointer-events: none; visibility: hidden; z-index: -1;"><div class="chartjs-size-monitor-expand" style="position:absolute;left:0;top:0;right:0;bottom:0;overflow:hidden;pointer-events:none;visibility:hidden;z-index:-1;"><div style="position:absolute;width:1000000px;height:1000000px;left:0;top:0"></div></div><div class="chartjs-size-monitor-shrink" style="position:absolute;left:0;top:0;right:0;bottom:0;overflow:hidden;pointer-events:none;visibility:hidden;z-index:-1;"><div style="position:absolute;width:200%;height:200%;left:0; top:0"></div></div></div>
                        <!-- Donut Chart Container -->
                        <canvas id="typeChart" class="js-chartjs2-donut chartjs-render-monitor" width="558" height="279" style="display: block; width: 558px; height: 279px;"></canvas>
                    </div>
                </div>
            </div>
        </div>
            <div class="row">
                <div class="col-lg-8 col-lg-offset-2">
                    <div class="block">
                        <div class="block-header">
                            <h3 class="block-title">Ticket Priority Statistics By Project</h3>
                        </div>
                        <div class="block-content block-content-full text-center">
                            <div class="chartjs-size-monitor" style="position: absolute; left: 0px; top: 0px; right: 0px; bottom: 0px; overflow: hidden; pointer-events: none; visibility: hidden; z-index: -1;"><div class="chartjs-size-monitor-expand" style="position:absolute;left:0;top:0;right:0;bottom:0;overflow:hidden;pointer-events:none;visibility:hidden;z-index:-1;"><div style="position:absolute;width:1000000px;height:1000000px;left:0;top:0"></div></div><div class="chartjs-size-monitor-shrink" style="position:absolute;left:0;top:0;right:0;bottom:0;overflow:hidden;pointer-events:none;visibility:hidden;z-index:-1;"><div style="position:absolute;width:200%;height:200%;left:0; top:0"></div></div></div>

                            <canvas id="projectChart" class="js-chartjs2-bars"></canvas>
                        </div>
                    </div>
                </div>
            </div>
        }
        <div class="row">
            @if (Model.OwnedTickets.Any())
            {
                <div class="col-lg-6">
                    <div class="block">
                        <div class="block-header">
                            <h3 class="block-title">My Submitted Tickets</h3>
                        </div>
                        <div class="block-content block-content-full bg-gray-lighter">

                            <div class="row">

                                <table class="table hoverTable table-striped viewTicketsModal" id="submittedTicketViewTable">
                                    <thead>
                                        <tr>
                                            <th style="word-wrap:break-word; width:15%">Title</th>
                                            <th class="hidden-xs" style="width:18%">Summary</th>
                                            <th style="width:15%">Priority</th>
                                            <th style="width:15%" class="hidden-xs">Status</th>
                                            <th style="width:12%" class="hidden-xs">Type</th>
                                            <th style="width:15%" class="hidden-xs">Created</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var ticket in Model.OwnedTickets)
                                        {

                                            <tr style="cursor:pointer" onclick="window.location.href = '@Url.Action("Details","Tickets", new { id=ticket.Id })';">
                                                <td style="word-wrap:break-word;">@ticket.Title</td>
                                                <td class="hidden-xs">@ticket.Summary</td>
                                                <td>
                                                    @switch (ticket.Priority.Name)
                                                    {
                                                        case "High":
                                                            <span class="label label-danger">High</span>
                                                            break;
                                                        case "Medium":
                                                            <span class="label label-warning">Medium</span>
                                                            break;
                                                        case "Low":
                                                            <span class="label label-info">Low</span>
                                                            break;
                                                    }
                                                </td>
                                                <td class="hidden-xs">

                                                    @switch (ticket.Status.Name)
                                                    {
                                                        case "Created":
                                                            <span class="label label-danger">Created</span>
                                                            break;
                                                        case "Assigned":
                                                            <span class="label label-warning">Assigned</span>
                                                            break;
                                                        case "Resolved":
                                                            <span class="label label-success">Resolved</span>
                                                            break;
                                                        case "Reopened":
                                                            <span class="label label-danger">Reopened</span>
                                                            break;
                                                        case "Archived":
                                                            <span class="label label-info">Archived</span>
                                                            break;
                                                    }
                                                </td>
                                                <td class="hidden-xs"><span class="label label-info">@ticket.Type.Name</span></td>

                                                <td class="hidden-xs">@ticket.CreatedDate</td>
                                            </tr>
                                        }
                                    </tbody>
                                </table>
                            </div>

                        </div>
                        <div class="block-content">
                        </div>
                    </div>
                </div>
            }
            @if (Model.AssignedProjects.Any())
            {
                <div class="col-lg-6">
                    <div class="block">
                        <div class="block-header">
                            <h3 class="block-title">My Assigned Projects</h3>
                        </div>
                        <div class="block-content block-content-full bg-gray-lighter">

                            <div class="row">

                                @foreach (var project in Model.AssignedProjects)
                                {
                                    <div class="col-sm-6 col-lg-6">
                                        <div class="block block-rounded block-themed">
                                            <div class="block-header bg-primary">
                                                <h3 class="h4 font-w600 push-5">
                                                    <a class="text-white" href="@Url.Action("Details","Projects",new { id=project.Id })">@project.Name</a>
                                                </h3>
                                                <h4 class="h5 text-white-op">@project.Description</h4>
                                            </div>
                                            <div class="block-content text-center">
                                                <div class="btn-group push">
                                                    <a class="btn btn-default js-tooltip" href="javascript:void(0)" title="Developers" data-toggle="modal" data-target="#viewDevelopersModal-@project.Id">
                                                        @project.Users.Count
                                                        <i class="fa fa-user-circle-o"></i>
                                                    </a>


                                                    <div class="modal fade" id="viewDevelopersModal-@project.Id" tabindex="-1" role="dialog" aria-hidden="true">
                                                        <div class="modal-dialog modal-lg modal-dialog-top">
                                                            <div class="modal-content">
                                                                <div class="block">
                                                                    <div class="block-content">

                                                                        <table class="table table-striped viewDevelopersTable" id="userManagmentTable">
                                                                            <thead>
                                                                                <tr>
                                                                                    <th style="width:15%">Name</th>
                                                                                    <th style="width:15%" class="hidden-xs">Email</th>
                                                                                    <th class="hidden-xs" style="width: 12%;">Assigned Tickets</th>
                                                                                    <th class="hidden-xs" style="width: 12%;">Assigned Projects</th>
                                                                                    <th class="hidden-xs" style="width: 15%;">Role</th>
                                                                                </tr>
                                                                            </thead>
                                                                            <tbody>
                                                                                @foreach (var user in project.Users)
                                                                                {

                                                                                    <tr>
                                                                                        <td class="font-w600">@user.FirstName @user.LastName</td>
                                                                                        <td class="hidden-xs">@user.Email</td>
                                                                                        <td class="hidden-xs">@user.AssignedTickets.Count</td>
                                                                                        <td class="hidden-xs">@user.Projects.Count</td>
                                                                                        <td class="hidden-xs">
                                                                                            @switch (user.Role)
                                                                                            {
                                                                                                case "Administrator":
                                                                                                    <span class="label label-danger">Administrator</span>
                                                                                                    break;
                                                                                                case "Project Manager":
                                                                                                    <span class="label label-warning">Project Manager</span>
                                                                                                    break;
                                                                                                case "Developer":
                                                                                                    <span class="label label-success">Developer</span>
                                                                                                    break;
                                                                                                case "Submitter":
                                                                                                    <span class="label label-primary">Submitter</span>
                                                                                                    break;
                                                                                                default:
                                                                                                    <span class="label label-info">UNKNOWN</span>
                                                                                                    break;
                                                                                            }
                                                                                        </td>
                                                                                    </tr>
                                                                                }
                                                                            </tbody>
                                                                        </table>
                                                                    </div>
                                                                </div>
                                                                <div class="modal-footer">
                                                                    <button class="btn btn-sm btn-default" type="button" data-dismiss="modal"><i class="fa fa-close push-5-r"></i>Close</button>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <a class="btn btn-default js-tooltip" data-toggle="modal" data-target="#viewAssignedTicketsModal-@project.Id" href="javascript:void(0)" title="View Assigned Tickets">
                                                        (@project.Tickets.Where(x => x.AssignedUserId == User.Identity.GetUserId()).Count()) <i class="fa fa-search"></i>
                                                    </a>

                                                    <a class="btn btn-default js-tooltip" data-toggle="modal" data-target="#viewTicketsModal-@project.Id" href="javascript:void(0)" title="View All Tickets">
                                                        (@project.Tickets.Count()) <i class="fa fa-files-o"></i>
                                                    </a>

                                                    <div class="modal fade " id="viewTicketsModal-@project.Id" tabindex="-1" role="dialog" aria-hidden="true">
                                                        <div class="modal-dialog modal-lg modal-dialog-top">
                                                            <div class="modal-content">
                                                                <div class="block">
                                                                    <div class="block-content">
                                                                        <table class="table table-striped hoverTable viewTicketsModal" id="userManagmentTable">
                                                                            <thead>
                                                                                <tr>
                                                                                    <th style="width:20%">Title</th>
                                                                                    <th style="width:45%">Summary</th>
                                                                                    <th class="hidden-xs" style="width: 10%;">Created Date</th>
                                                                                    <th class="hidden-xs" style="width: 10%;">Updated Date</th>
                                                                                    <th class="hidden-xs" style="width: 5%;">Priority</th>
                                                                                    <th class="hidden-xs" style="width: 5%;">Status</th>
                                                                                    <th class="hidden-xs" style="width: 5%;">Type</th>
                                                                                </tr>
                                                                            </thead>
                                                                            <tbody>
                                                                                @foreach (var ticket in project.Tickets)
                                                                                {

                                                                                    <tr style="cursor:pointer" onclick="window.location.href = '@Url.Action("Details","Tickets", new { id=ticket.Id })';">
                                                                                        <td class="font-w600">@ticket.Title</td>
                                                                                        <td>@ticket.Summary</td>
                                                                                        <td class="hidden-xs">@ticket.CreatedDate</td>
                                                                                        <td class="hidden-xs">@ticket.UpdatedDate</td>
                                                                                        <td class="hidden-xs">
                                                                                            @switch (ticket.Priority.Name)
                                                                                            {
                                                                                                case "High":
                                                                                                    <span class="label label-danger">High</span>
                                                                                                    break;
                                                                                                case "Medium":
                                                                                                    <span class="label label-warning">Medium</span>
                                                                                                    break;
                                                                                                case "Low":
                                                                                                    <span class="label label-info">Low</span>
                                                                                                    break;
                                                                                            }
                                                                                        </td>
                                                                                        <td class="hidden-xs">

                                                                                            @switch (ticket.Status.Name)
                                                                                            {
                                                                                                case "Created":
                                                                                                    <span class="label label-danger">Created</span>
                                                                                                    break;
                                                                                                case "Assigned":
                                                                                                    <span class="label label-warning">Assigned</span>
                                                                                                    break;
                                                                                                case "Resolved":
                                                                                                    <span class="label label-success">Resolved</span>
                                                                                                    break;
                                                                                                case "Reopened":
                                                                                                    <span class="label label-danger">Reopened</span>
                                                                                                    break;
                                                                                                case "Archived":
                                                                                                    <span class="label label-info">Archived</span>
                                                                                                    break;
                                                                                            }
                                                                                        </td>
                                                                                        <td class="hidden-xs"><span class="label label-info">@ticket.Type.Name</span></td>
                                                                                    </tr>
                                                                                }
                                                                            </tbody>
                                                                        </table>

                                                                    </div>
                                                                </div>
                                                                <div class="modal-footer">
                                                                    <button class="btn btn-sm btn-default" type="button" data-dismiss="modal"><i class="fa fa-close push-5-r"></i>Close</button>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="modal fade " id="viewAssignedTicketsModal-@project.Id" tabindex="-1" role="dialog" aria-hidden="true">
                                                        <div class="modal-dialog modal-lg modal-dialog-top">
                                                            <div class="modal-content">
                                                                <div class="block">
                                                                    <div class="block-content">
                                                                        <table class="table table-striped hoverTable viewTicketsModal" id="userManagmentTable">
                                                                            <thead>
                                                                                <tr>
                                                                                    <th style="width:20%">Title</th>
                                                                                    <th style="width:45%">Summary</th>
                                                                                    <th class="hidden-xs" style="width: 10%;">Created Date</th>
                                                                                    <th class="hidden-xs" style="width: 10%;">Updated Date</th>
                                                                                    <th class="hidden-xs" style="width: 5%;">Priority</th>
                                                                                    <th class="hidden-xs" style="width: 5%;">Status</th>
                                                                                    <th class="hidden-xs" style="width: 5%;">Type</th>
                                                                                </tr>
                                                                            </thead>
                                                                            <tbody>
                                                                                @foreach (var ticket in project.Tickets.Where(x => x.AssignedUserId == User.Identity.GetUserId()))
                                                                                {

                                                                                    <tr style="cursor:pointer" onclick="window.location.href = '@Url.Action("Details","Tickets", new { id=ticket.Id })';">
                                                                                        <td class="font-w600">@ticket.Title</td>
                                                                                        <td>@ticket.Summary</td>
                                                                                        <td class="hidden-xs">@ticket.CreatedDate</td>
                                                                                        <td class="hidden-xs">@ticket.UpdatedDate</td>
                                                                                        <td class="hidden-xs">
                                                                                            @switch (ticket.Priority.Name)
                                                                                            {
                                                                                                case "High":
                                                                                                    <span class="label label-danger">High</span>
                                                                                                    break;
                                                                                                case "Medium":
                                                                                                    <span class="label label-warning">Medium</span>
                                                                                                    break;
                                                                                                case "Low":
                                                                                                    <span class="label label-info">Low</span>
                                                                                                    break;
                                                                                            }
                                                                                        </td>
                                                                                        <td class="hidden-xs">

                                                                                            @switch (ticket.Status.Name)
                                                                                            {
                                                                                                case "Created":
                                                                                                    <span class="label label-danger">Created</span>
                                                                                                    break;
                                                                                                case "Assigned":
                                                                                                    <span class="label label-warning">Assigned</span>
                                                                                                    break;
                                                                                                case "Resolved":
                                                                                                    <span class="label label-success">Resolved</span>
                                                                                                    break;
                                                                                                case "Reopened":
                                                                                                    <span class="label label-danger">Reopened</span>
                                                                                                    break;
                                                                                                case "Archived":
                                                                                                    <span class="label label-info">Archived</span>
                                                                                                    break;
                                                                                            }
                                                                                        </td>
                                                                                        <td class="hidden-xs"><span class="label label-info">@ticket.Type.Name</span></td>
                                                                                    </tr>
                                                                                }
                                                                            </tbody>
                                                                        </table>

                                                                    </div>
                                                                </div>
                                                                <div class="modal-footer">
                                                                    <button class="btn btn-sm btn-default" type="button" data-dismiss="modal"><i class="fa fa-close push-5-r"></i>Close</button>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                </div>
                                                <ul class="list-inline push">
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                }
                            </div>

                        </div>
                        <div class="block-content">
                        </div>
                    </div>
                </div>
            }
            @if (Model.AssignedProjects.Any(x => x.Tickets.Any(z => z.AssignedUserId == User.Identity.GetUserId())))
            {
                <div class="col-lg-6">
                    <div class="block">
                        <div class="block-header">
                            <h3 class="block-title">My Assigned Tickets</h3>
                        </div>
                        <div class="block-content block-content-full bg-gray-lighter">

                            <div class="row">

                                <table class="table  table-striped hoverTable" id="ticketViewTable">
                                    <thead>
                                        <tr>
                                            <th style="word-wrap:break-word; width:15%">Title</th>
                                            <th class="hidden-xs" style="width:18%">Summary</th>
                                            <th style="width:15%">Priority</th>
                                            <th style="width:15%" class="hidden-xs">Status</th>
                                            <th style="width:12%" class="hidden-xs">Type</th>
                                            <th style="width:15%" class="hidden-xs">Created</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var project in Model.AssignedProjects.Where(project => project.Tickets.Any()))
                                        {
                                            foreach (var ticket in project.Tickets.Where(x => x.AssignedUserId == User.Identity.GetUserId()))
                                            {

                                                <tr style="cursor:pointer" onclick="window.location.href = '@Url.Action("Details","Tickets", new { id=ticket.Id })';">
                                                    <td>@ticket.Title</td>
                                                    <td class="hidden-xs">@ticket.Summary</td>
                                                    <td>
                                                        @switch (ticket.Priority.Name)
                                                        {
                                                            case "High":
                                                                <span class="label label-danger">High</span>
                                                                break;
                                                            case "Medium":
                                                                <span class="label label-warning">Medium</span>
                                                                break;
                                                            case "Low":
                                                                <span class="label label-info">Low</span>
                                                                break;
                                                        }
                                                    </td>
                                                    <td class="hidden-xs">

                                                        @switch (ticket.Status.Name)
                                                        {
                                                            case "Created":
                                                                <span class="label label-danger">Created</span>
                                                                break;
                                                            case "Assigned":
                                                                <span class="label label-warning">Assigned</span>
                                                                break;
                                                            case "Resolved":
                                                                <span class="label label-success">Resolved</span>
                                                                break;
                                                            case "Reopened":
                                                                <span class="label label-danger">Reopened</span>
                                                                break;
                                                            case "Archived":
                                                                <span class="label label-info">Archived</span>
                                                                break;
                                                        }
                                                    </td>
                                                    <td class="hidden-xs"><span class="label label-info">@ticket.Type.Name</span></td>

                                                    <td class="hidden-xs">@ticket.CreatedDate</td>
                                                </tr>
                                            }
                                        }
                                    </tbody>
                                </table>
                            </div>

                        </div>
                        <div class="block-content">
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
</main>
@section scripts{
    <script src="~/Scripts/core/Chart.min.js"></script>
    <script>
        $(document).ready(function () {
            $('#ticketViewTable').DataTable();
            $('.viewDevelopersTable').DataTable();
            $(".viewTicketsModal").DataTable();
        });
    </script>
    <script>

        Chart.defaults.global.defaultFontColor              = '#999';
        Chart.defaults.global.defaultFontFamily             = 'Open Sans';
        Chart.defaults.global.defaultFontStyle              = '600';
        Chart.defaults.scale.gridLines.color               = "rgba(0,0,0,.05)";
        Chart.defaults.scale.gridLines.zeroLineColor       = "rgba(0,0,0,.1)";
        Chart.defaults.global.elements.line.borderWidth     = 2;
        Chart.defaults.global.elements.point.radius         = 4;
        Chart.defaults.global.elements.point.hoverRadius    = 6;
        Chart.defaults.global.tooltips.titleFontFamily      = 'Source Sans Pro';
        Chart.defaults.global.tooltips.bodyFontFamily       = 'Open Sans';
        Chart.defaults.global.tooltips.cornerRadius         = 3;
        Chart.defaults.global.legend.labels.boxWidth = 15;


        var labels = [];
        var lowPriorityAmounts = []
        var mediumPriorityAmounts = []
        var highPriorityAmounts = []
        
        @foreach (var project in Model.Projects) {
            @:labels.push("@project.Name");
            @:lowPriorityAmounts.push("@project.Tickets.Where(x=>x.Priority.Name=="Low").Count()");
            @:mediumPriorityAmounts.push("@project.Tickets.Where(x=>x.Priority.Name=="Medium").Count()");
            @:highPriorityAmounts.push("@project.Tickets.Where(x=>x.Priority.Name=="High").Count()");
        }

        var projectInformation = {
            labels: labels,
            datasets: [
                {
                label: 'High Priority',
                    fill: true,
                    backgroundColor: 'rgba(220,220,220,.3)',
                    borderColor: 'rgba(220,220,220,1)',
                    pointBackgroundColor: 'rgba(220,220,220,1)',
                    pointBorderColor: '#fff',
                    pointHoverBackgroundColor: '#fff',
